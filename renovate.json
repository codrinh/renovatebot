{
    "extends": [
      "config:base"
    ],
    "registryUrls": [
      "https://hub.docker.com"
    ],
    "packageRules": [
      {
        "matchPackageNames": [
          "match-depName-below"
        ],
        "versioning": "regex:^team-(?<major>\\d+)(\\.(?<minor>\\d+))?(\\.(?<patch>\\d+))-plugins$"
      }
    ],
    "regexManagers": [
      {
        "description": "Yet another regex",
        "fileMatch": "mypath/chart.yaml$",
        "matchStringsStrategy": "combination",
        "matchStrings": [
          "image: \"(?<registryUrl>hub.docker.com\/?)(?<depName>.*?)\"\n",
          "tag: \"?(?<currentValue>.*)?\""
        ],
        "datasourceTemplate": "docker"
      }
    ]
  }